{
  "name": "stiqr-monorepo",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "build": "pnpm --filter frontend build",
    "preview": "pnpm --filter frontend preview",
    "typecheck": "pnpm --filter frontend typecheck",
    "lint": "pnpm --filter frontend lint",
    "lint:check": "pnpm --filter frontend lint:check",
    "format": "pnpm --filter frontend format",
    "format:check": "pnpm --filter frontend format:check",
    "check": "pnpm --filter frontend check",
    "dev:all": "pnpm --parallel dev",
    "build:all": "pnpm -r build",
    "frontend": "pnpm --filter frontend",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5"
  }
}
